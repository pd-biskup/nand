Name
    NAND4B
;
Description
    4-bit NAND.
;
Inputs
    in1[4], in2[4]
;
Outputs
    out[4]
;
Chips
    nand1 NAND, nand2 NAND, nand3 NAND, nand4 NAND
;
Wires
    in1[1] -> nand1.in1,
    in2[1] -> nand1.in2,
    nand1.out -> out[1],
    in1[2] -> nand2.in1,
    in2[2] -> nand2.in2,
    nand2.out -> out[2],
    in1[3] -> nand3.in1,
    in2[3] -> nand3.in2,
    nand3.out -> out[3],
    in1[4] -> nand4.in1,
    in2[4] -> nand4.in2,
    nand4.out -> out[4]
;